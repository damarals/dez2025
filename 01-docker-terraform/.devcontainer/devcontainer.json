{
	"name": "DEZ 2025 - Docker & Terraform",
	"dockerComposeFile": [
        "../docker-compose.yaml",
        "docker-compose.yaml"
    ],
    "service": "app",
	"workspaceFolder": "/home/zoomcamp/app",
	"remoteUser": "zoomcamp",
	"features": {
		"ghcr.io/devcontainers/features/common-utils:2": {},
		"ghcr.io/stuartleeks/dev-container-features/shell-history:0": {},
		"ghcr.io/nils-geistmann/devcontainers-features/zsh:0": {}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-toolsai.jupyter",
				"hashicorp.terraform"
			]
		}
	}
}